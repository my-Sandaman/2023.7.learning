    "import sympy as sp#符号解\n",
    "\n",
    "x, y = sp.symbols('x y')\n",
    "\n",
    "eq1 = x**2 - y - x - 3\n",
    "eq2 = x + 3*y -2\n",
    "\n",
    "sol = sp.solve([eq1, eq2], [x,y])\n",
    "\n",
    "print(sol)"
