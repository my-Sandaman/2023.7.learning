   "source": [
    "from scipy.optimize import fsolve,root\n",
    "fx = lambda x :[x[0]**2-1,x[0]-x[1]]\n",
    "s1 = fsolve(fx,[1,1])\n",
    "s2 = root(fx,[1,1])\n",
    "print(s1,'/n','---------')\n",
    "print(s2)"
   ]
